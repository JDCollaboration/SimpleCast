    "@types/express": "^4.0.39",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.56",
    "@types/pg": "^7.4.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.5",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2"
